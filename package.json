{
  "name": "@pelevesque/canonicalize-string",
  "version": "0.0.6",
  "description": "Canonicalizes a UTF-8 string.",
  "main": "index.js",
  "engines": {
    "node": ">=9.0.0"
  },
  "eslintConfig": {
    "globals": {
      "describe": "readonly",
      "it": "readonly"
    },
    "rules": {
      "key-spacing": [
        "error",
        {
          "mode": "minimum"
        }
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline"
        }
      ],
      "no-multi-spaces": [
        "error",
        {
          "exceptions": {
            "ObjectExpression": true,
            "ArrayExpression": true,
            "IfStatement": true
          }
        }
      ]
    }
  },
  "scripts": {
    "test": "standard && mocha --reporter spec && node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*",
    "standard": "standard",
    "unit": "mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pelevesque/canonicalize-string.git"
  },
  "keywords": [
    "UTF-8",
    "string",
    "canonicalize",
    "canonical",
    "homoglyph",
    "diacritic"
  ],
  "author": "Pierre-Emmanuel LÃ©vesque <pierre.e.levesque@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pelevesque/canonicalize-string/issues"
  },
  "homepage": "https://github.com/pelevesque/canonicalize-string#readme",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.1.0",
    "standardx": "^5.0.0"
  },
  "dependencies": {
    "diacritics": "^1.3.0",
    "unhomoglyph": "^1.0.4"
  }
}
